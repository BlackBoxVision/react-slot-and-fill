{
  "name": "react-slot-and-fill",
  "version": "1.0.0",
  "private": false,
  "description": "React Slot and Fill implementation made with React.createContext API",
  "umd": {
    "name": "ReactSlotAndFill",
    "globals": {
      "react": "React",
      "prop-types": "PropTypes"
    }
  },
  "external": [
    "react",
    "prop-types"
  ],
  "main": "lib/react-slot-and-fill.cjs.js",
  "module": "lib/react-slot-and-fill.esm.js",
  "browser": "lib/react-slot-and-fill.umd.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "link": "npm run build && npm link",
    "clean": "rimraf ./lib",
